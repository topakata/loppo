  	<servlet-name>RegistrationServlet</servlet-name>
  	<url-pattern>/RegistrationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CatsServlet</servlet-name>
  	<url-pattern>/CatsServlet</url-pattern>
  </servlet-mapping>
